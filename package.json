{
  "name": "@quimikuaxh/webscrap",
  "version": "1.0.5",
  "description": "Library to easily webscrap and download images",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm i && tsc --project tsconfig.json && tscpaths -p tsconfig.json -s ./src -o ./dist",
    "clean": "rm -rf node_modules dist package-lock.json || true",
    "clean-build": "npm run clean && npm run build",
    "lint": "eslint . --ext .ts",
    "publish": "npm publish --access=public",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "quimikuaxh",
  "license": "ISC",
  "files": [
    "dist/*"
  ],
  "dependencies": {
    "@types/node": "20.8.9",
    "@typescript-eslint/eslint-plugin": "6.9.0",
    "@typescript-eslint/parser": "6.9.0",
    "axios": "1.6.0",
    "eslint": "8.52.0",
    "request": "2.88.2",
    "ts-node": "10.9.1",
    "tscpaths": "^0.0.9",
    "xpath-html": "1.0.3"
  },
  "devDependencies": {
    "@types/request": "2.48.11",
    "tsconfig-paths": "^4.2.0"
  }
}
